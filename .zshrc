# Starship(https://starship.rs/)
# auto start
eval "$(starship init zsh)"

# Zellij(http://zellij.dev/)
# auto start
eval "$(zellij setup --generate-auto-start zsh)"

# alias
alias ll='ls -l'
alias la='ls -a'
alias lla='ls -la'
alias yzi='yazi'
alias lg='lazygit'
alias cl='clear'

# misc
export EDITOR=hx
